{
  "name": "pesquisa",
  "version": "0.1.0",
  "bin": {
    "pesquisa": "bin/pesquisa.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc -b -clean",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cd lambda && npm run build && cd .. && npm run build && cdk deploy && npm run clean",
    "run-same-local-fe-api": "sam local start-api -p 4000 -t ./template.yaml",
    "dev": "npm run build && npm run synth && concurrently --kill-others \"npm run watch\" \"npm run run-same-local-fe-api\"",
    "synth": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "20.14.9",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "aws-cdk": "2.154.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.645.0",
    "@aws-sdk/client-sns": "^3.645.0",
    "aws-cdk-lib": "2.154.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.5",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "source-map-support": "^0.5.21",
    "ts-custom-error": "^3.3.1",
    "zod": "^3.23.8"
  }
}
